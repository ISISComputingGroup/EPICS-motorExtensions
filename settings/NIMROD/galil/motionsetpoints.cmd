$(IFIOC_GALIL_04)
$(IFIOC_GALIL_04) dbLoadRecords("$(MOTIONSETPOINTS)/db/motionSetPointsSingleAxis.db","P=$(MYPVPREFIX)CAMERA:,NAME0=Position,AXIS0=$(MYPVPREFIX)MOT:CAM,TOL=1,LOOKUP=LOOKUP_CAM")
